<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.software.hereisdog.dao.PlaceDao">
    <select id="findByType" resultType="com.software.hereisdog.domain.Place">
        SELECT * FROM places
        WHERE type = #{type}
    </select>
</mapper>
